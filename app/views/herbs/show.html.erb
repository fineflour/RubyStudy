<div class="col-md-10 col-xs-12 col-sm-9">
  <div id="user-show">
    <div class="page-header">
      <h3><%= @herb.name_eng %></h3>
    </div>

    <div class="row">
      <div class="col-md-10">
        <div><%= render partial: "field", locals: { herb: @herb} %></div>
        <div class="button-padding pull-right">
          <%= link_to "Back to index", herbs_path, class: "btn btn-default" %>
          <%= link_to "Edit", edit_herb_path(@herb), class: "btn btn-info" %>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-md-6">
        < @indications.each do |i| %>
          <= i.name_eng %>
      < end %>
      </div>
    </div>
  </div>
