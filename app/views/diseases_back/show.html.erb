<div class="right-panel">
    <div class="page-header">
      <h3><%= @disease.name_eng %></h3>
    </div>
    <div class="row">
      <div class="col-md-10">
        <div><%= render partial: "field", locals: { disease: @disease, subindications: @subindications, category: @category} %></div>
        <div class="button-padding pull-right">
          <%= link_to "Back to index", diseases_path, class: "btn btn-default" %>
          <%= link_to "Edit", edit_disease_path(@disease), class: "btn btn-info" %>
        </div>
      </div>
    </div>
