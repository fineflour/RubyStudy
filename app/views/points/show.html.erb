<div class="col-md-10 col-xs-12 col-sm-9">
  <div id="-show">
    <div class="page-header">
      <h3><%= @point.name_eng %></h3>
    </div>

    <div class="row">
      <div class="col-md-12">
        <div><%= render partial: "field", locals: {point: @point} %></div>
        <div class="button-padding pull-right">
          <%= link_to "Back to index", points_path, class: "btn btn-default" %>
          <%= link_to "Edit", edit_point_path(@point), class: "btn btn-info" %>
        </div>
      </div>
    </div>
</div>
